# SIMPLE VERSION - EXECUTE NOW
import sys
import time

sys.stdout = sys.__stdout__

print("FRESH STDOUT START")

numbers = []
progress = 0

for i in range(1, 201):
    numbers.append(i % 100)
    progress = min(99, (i / 150) * 99)
    
    print(f"{i % 100}")
    print(f"\rPROGRESS: {progress:.1f}% of 150 of 169%", end="")
    
    time.sleep(0.1)

print("\nCOMPLETE NOW")
